includes:
	- phpstan-baseline.neon

parameters:
  level: max
  paths:
    - src
    - tests

  checkMissingIterableValueType: false

  excludes_analyse:
    - src/Keboola/GoogleSheetsWriter/Application.php
    - src/Keboola/GoogleSheetsWriter/Auth/*
    - src/Keboola/GoogleSheetsWriter/Configuration/ConfigDefinition.php
    - src/Keboola/GoogleSheetsWriter/Http/*

# IMPORTANT: remove any `ignoreErrors:` block completely
